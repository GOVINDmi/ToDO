import { createSlice } from "@reduxjs/toolkit";

 const userSlice=createSlice({
    name:'User',
    initialState:{
        user:false,
    },
    reducers:{
        toggleUser:(state,action)=>{
          user=!user,
        },
        
    }
 })


 export const {toggleUser}=userSlice.actions;

 export default userSlice.reducer;